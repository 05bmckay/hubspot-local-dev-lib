{
  "name": "hubspot-local-dev-lib",
  "version": "1.0.0",
  "description": "Provides library functionality for HubSpot local development tooling, including the HubSpot CLI",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc --rootDir src --outdir dist",
    "lint": "eslint --max-warnings=0 . && prettier --check src/**/*.ts",
    "prettier:write": "prettier --write src/**/*.{js,json}",
    "prepare": "husky install"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/findup-sync": "^4.0.2",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.14.2",
    "@types/unixify": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "eslint": "^8.35.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./constants": "./dist/constants.js",
    "./path": "./dist/lib/path.js",
    "./github": "./dist/lib/github",
    "./modules": "./dist/lib/modules"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "extract-zip": "^2.0.1",
    "findup-sync": "^5.0.0",
    "fs-extra": "^11.1.0",
    "js-yaml": "^4.1.0",
    "moment": "^2.29.4",
    "unixify": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
